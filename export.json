    {
      "parameters": {
        "jsCode": "// n8n Code Node\nreturn items.map(item => {\n  let output = item.json.output;\n\n  // First parse: item.json.output might be embedded JSON string\n  let parsed;\n  try {\n    parsed = JSON.parse(output);\n  } catch (e) {\n    throw new Error(\"Failed to parse LLM output JSON: \" + e.message);\n  }\n\n  // Expecting structure { post: \"...\", image: \"base64...\" }\n  const post = parsed.post?.trim() || \"\";\n  const imagePrompt = parsed.imagePrompt || null;\n\n  return {\n    json: {\n      post,\n      imagePrompt\n    }\n  };\n});"
      },
